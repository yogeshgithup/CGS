<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <listener-class>
            DatabaseUtil.MyHibernateConnection
        </listener-class>
    </listener>
    <servlet>
        <servlet-name>Aaddpackage</servlet-name>
        <servlet-class>Allservlet.Aaddpackage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Aaddgym</servlet-name>
        <servlet-class>Allservlet.Aaddgym</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Viewpackage</servlet-name>
        <servlet-class>Allservlet.Viewpackage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loginverify</servlet-name>
        <servlet-class>Allservlet.Loginverify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Practise</servlet-name>
        <servlet-class>Allservlet.Practise</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Loginverify</servlet-name>
        <url-pattern>/Loginverify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Aaddpackage</servlet-name>
        <url-pattern>/Aaddpackage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Aaddgym</servlet-name>
        <url-pattern>/Aaddgym</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Viewpackage</servlet-name>
        <url-pattern>/Viewpackage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Practise</servlet-name>
        <url-pattern>/Practise</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
